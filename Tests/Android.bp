cc_library_shared {
    name: "libTestSetupUtils",
    defaults:["resource_tuner_defaults"],
    srcs: ["Utils/*.cpp"],
    shared_libs: [
        "libAuxiliary",
        "libyaml",
        "liblog",
    ],
    header_libs: ["libyaml_headers"],
    export_header_lib_headers: ["libyaml_headers"],
    local_include_dirs: ["Utils/Include"],
    export_include_dirs: ["Utils/Include"],
    installable: true,
}

